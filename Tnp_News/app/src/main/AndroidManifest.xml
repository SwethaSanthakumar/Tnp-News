<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tnpnewsapp">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
         <activity android:name=".Analysis" />
        <activity android:name=".Settings" />
        <activity android:name=".App" />
        <activity android:name=".Feedback" />
        <activity android:name=".Gps" />
        <activity android:name=".Noti"/>
        <activity android:name=".Home" />
        <activity android:name=".Profile" />
        <activity android:name=".PrefsActivity" />
        <activity android:name=".TaskHome" />
        <activity android:name=".AddTask" />
        <activity android:name=".BackgroundNotificationService" />
        <activity android:name=".Wifi" />
        <activity android:name=".LoginForm"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>